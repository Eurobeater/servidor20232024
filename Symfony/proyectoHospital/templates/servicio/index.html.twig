{# plantilla/prueba.html.twig' #}
{% extends 'default.html.twig' %}
{% block contenido %}
<h1>Servicios por especialidad</h1>

<table>
    <tr>
    	<th>Especialidad</th>
	</tr>
	{% for especialidad in especialidades %}
        <tr>
			<td><a href="{{ path('app_servicio_detalles', { id: especialidad.id }) }}">{{especialidad.nombre}}</a></td>
		</tr>
    {% endfor %}
</table>

{% endblock %}
{% block right %}
{% include 'derecha.html.twig'%}
{% endblock %}